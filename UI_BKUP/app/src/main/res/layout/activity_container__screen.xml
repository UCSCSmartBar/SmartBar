<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginEnd="0dp"
    android:orientation="vertical"
    android:background="#440099"
    >
<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
        <ScrollView
            android:layout_width="505dp"
            android:layout_height="550dp"
            android:background="@drawable/inv_backdrop"
            >

        <GridLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="0dp"
            android:layout_marginBottom="30dp"
            android:paddingBottom="@dimen/activity_vertical_margin"
            tools:context="com.example.trider.smartbarui.Container_Screen">
            <Button
                style="@style/Container"
                android:id="@+id/con1_button"
                android:onClick="onContainerClick"
                android:text="Container One"
                android:layout_row="0"
                android:layout_column="0"
                />
            <Button
                style="@style/Container"
                android:id="@+id/con2_button"
                android:onClick="onContainerClick"
                android:text="Container Two"
                android:layout_column="1"
                android:layout_row="0"
                />
            <Button
                style="@style/Container"
                android:id="@+id/con3_button"
                android:onClick="onContainerClick"
                android:text="Container Three"
                android:layout_row="1"
                android:layout_column="0"
                />
            <Button
                style="@style/Container"
                android:id="@+id/con4_button"
                android:onClick="onContainerClick"
                android:text="Container Four"
                android:layout_row="1"
                android:layout_column="1"
                />
            <Button
                style="@style/Container"
                android:id="@+id/con5_button"
                android:onClick="onContainerClick"
                android:text="Container Five"
                android:layout_row="2"
                android:layout_column="0"

               />
            <!-- Water Containers -->
            <Button
                style="@style/WaterContainer"
                android:id="@+id/con6_button"
                android:onClick="onContainerClick"
                android:text="Container Six"
                android:layout_row="2"
                android:layout_column="1"
              />
            <Button
                style="@style/WaterContainer"
                android:id="@+id/con7_button"
                android:onClick="onContainerClick"
                android:text="Container Seven"
                android:layout_row="3"
                android:layout_column="0"
                />
            <!-- -->
            <Button
                style="@style/Container"
                android:id="@+id/con8_button"
                android:onClick="onContainerClick"
                android:text="Container Eight"
                android:layout_row="3"
                android:layout_column="1"
              />
            <Button
                style="@style/Container"
                android:id="@+id/con9_button"
                android:onClick="onContainerClick"
                android:text="Container Nine"
                android:layout_row="4"
                android:layout_column="0"
              />
            <Button
                style="@style/Container"
                android:id="@+id/con10_button"
                android:onClick="onContainerClick"
                android:text="Container Ten"
                android:layout_column="1"
                android:layout_row="4"
             />
            <Button
                style="@style/Container"
                android:id="@+id/con11_button"
                android:onClick="onContainerClick"
                android:text="Container Eleven"
                android:layout_row="5"
                android:layout_column="0"
               />
            <Button
                style="@style/Container"
                android:id="@+id/con12_button"
                android:onClick="onContainerClick"
                android:text="Container Twelve"
                android:layout_column="1"
                android:layout_row="5"
              />
            <Button
                style="@style/Container"
                android:id="@+id/con13_button"
                android:onClick="onContainerClick"
                android:text="Container Thirteen"
                android:layout_row="6"
                android:layout_column="0"
              />
            <Button
                style="@style/Container"
                android:id="@+id/con14_button"
                android:onClick="onContainerClick"
                android:text="Container Fourteen"
                android:layout_row="6"
                android:layout_column="1"
                />
            <Button
                style="@style/Container"
                android:id="@+id/con15_button"
                android:onClick="onContainerClick"
                android:text="Container Fifteen"
                android:layout_row="7"
                android:layout_column="0"
                />
            <Button
                style="@style/Container"
                android:id="@+id/con16_button"
                android:onClick="onContainerClick"
                android:text="Container Sixteen"
                android:layout_row="7"
                android:layout_column="1"
               />
            <Button
                style="@style/Container"
                android:id="@+id/con17_button"
                android:onClick="onContainerClick"
                android:text="Container Seventeen"
                android:layout_row="8"
                android:layout_column="0"
                />
            <Button
                style="@style/Container"
                android:id="@+id/con18_button"
                android:onClick="onContainerClick"
                android:text="Container Eighteen"
                android:layout_row="8"
                android:layout_column="1"
               />

            </GridLayout>
        </ScrollView>


    <LinearLayout
        android:layout_width="500dp"
        android:layout_height="600dp"
        android:background="#440099"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation = "horizontal"
            >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:layout_marginStart="10dp"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:text="Liquid"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:layout_marginStart="50dp"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:text="Brand"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:textAllCaps="true"
            android:textSize="20sp"
            android:textColor="@color/white"
            android:layout_marginStart="10dp"
            android:text="Vol(oz)"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_marginTop="80dp"
                android:textAllCaps="true"
                android:textSize="20sp"
                android:textColor="@color/white"
                android:layout_marginStart="10dp"
                android:text="$/oz"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="80dp"
                android:textAllCaps="true"
                android:textSize="20sp"
                android:textColor="@color/white"
                android:layout_marginStart="10dp"
                android:text="Carb"/>

        </LinearLayout>
        <View
            android:layout_width="500dp"
            android:layout_height="1dp"
            android:background="#ff000000"
            />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            >
            <NumberPicker
                android:id="@+id/numberPicker"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textColor="@color/white"/>

            <NumberPicker
                android:id="@+id/numberPicker2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textColor="@color/white"/>
            <NumberPicker
                android:id="@+id/numberPicker3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textColor="@color/white"/>
            <NumberPicker
                android:id="@+id/numberPicker4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textColor="@color/white"/>

            <NumberPicker
                android:id="@+id/numberPicker5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textColor="@color/white"/>

        </LinearLayout>
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Replace Container"
            style="@style/PinKey"
            android:onClick="ReplaceContainer"
            android:id="@+id/button22" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Click Container for Info"
            android:textColor="#ffffff"
            android:id="@+id/cont_tview"/>
<GridLayout
    android:layout_width="600dp"
    android:layout_height="200dp">
        <Button
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:text="Update Inventory"
            android:onClick = "SendInventory"
            android:id="@+id/button23"
            android:padding="5dp"
            style="@style/PinKey"
             />

        <Button
            style="@style/PinKey"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="Prime Container"
            android:onClick="PrimeContainer"
            android:id="@+id/button8"
             />

        <Button
            android:layout_column="0"
            android:layout_row="1"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:text="Main Menu"
            android:onClick = "BackToMain"
            android:id="@+id/cs_back"
            android:padding="0dp"
            style="@style/PinKey"
            />

        <Button
            android:layout_column="1"
            android:layout_row="1"
            style="@style/PinKey"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="Purge Container"
            android:onClick="PurgeContainer"
            android:id="@+id/button9"
             />

    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        style="@style/PinKey"
        android:text="Test Valves"
        android:id="@+id/button10"
        android:layout_row="0"
        android:onClick="TestValves"
        android:layout_column="2" />
</GridLayout>
    </LinearLayout>

</LinearLayout>




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="0dp"
        android:textColor="#ff000000"

        android:orientation="horizontal">





    </LinearLayout>

